{
  "name": "react-stdio",
  "version": "3.3.0",
  "description": "Render React.js components on any backend",
  "author": "Michael Jackson",
  "license": "MIT",
  "bin": "./bin/react-stdio",
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ReactTraining/react-stdio"
  },
  "files": ["bin", "server.js"],
  "scripts": {
    "release": "node ./scripts/release.js",
    "test": "jest",
    "dist:binary":
      "pkg ./bin/react-stdio -t $npm_package_config_platform -o ./dist/$npm_package_config_version/$npm_package_config_platform/react-stdio"
  },
  "dependencies": {
    "JSONStream": "^1.0.7",
    "event-stream": "^3.3.2",
    "invariant": "^2.2.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "jest": "^22.1.4",
    "pkg": "^4.3.0",
    "readline-sync": "^1.4.1"
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "__tests__/mocks"]
  }
}
